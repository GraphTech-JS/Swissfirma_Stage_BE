{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "date",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "video": {
      "type": "string"
    },
    "markdown_article": {
      "type": "richtext",
      "required": true
    },
    "faqs": {
      "type": "component",
      "component": "articles-components.faq-item",
      "repeatable": true,
      "required": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "articles"
    }
  }
}
