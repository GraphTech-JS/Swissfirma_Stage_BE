{
  "kind": "collectionType",
  "collectionName": "consultations",
  "info": {
    "singularName": "consultation",
    "pluralName": "consultations",
    "displayName": "Consultation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true,
      "regex": "^\\+\\d{10,15}$"
    }
  }
}
